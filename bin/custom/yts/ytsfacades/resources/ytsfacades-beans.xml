<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;com.yts.facades.product.data.GenderData>"/>
	</bean>

	<bean class="com.yts.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>

	<bean class = "com.yts.facades.concerttours.data.TourSummaryData">
		<description >Data object for a tour summary which has no equivalent on the type system</description>
		<property name = "id" type = "String" />
		<property name = "tourName" type = "String" />
		<property name = "numberOfConcerts" type = "String" />
	</bean>
	<bean class = "com.yts.facades.concerttours.data.BandData" >
		<description>Data object representing a Band</description>
		<property name = "id" type = "String" />
		<property name = "name" type = "String" />
		<property name = "description" type = "String" />
		<property name = "albumsSold" type = "Long" />
		<property name = "genres" type="java.util.List&lt;String&gt;"/>
		<property name = "tours" type="java.util.List&lt;com.yts.facades.concerttours.data.TourSummaryData&gt;"/>
	</bean>
	<bean class = "com.yts.facades.concerttours.data.ConcertSummaryData">
		<description >Data object for a concert summary</description>
		<property name = "id" type = "String" />
		<property name = "date" type = "java.util.Date" />
		<property name = "venue" type = "String" />
		<property name = "type" type = "String" />
	</bean>
	<bean class = "com.yts.facades.concerttours.data.TourData" >
		<description>Data object representing a tour</description>
		<property name = "id" type = "String" />
		<property name = "tourName" type = "String" />
		<property name = "description" type = "String" />
		<property name = "concerts" type="java.util.List&lt;com.yts.facades.concerttours.data.ConcertSummaryData&gt;"/>
	</bean>
</beans>
